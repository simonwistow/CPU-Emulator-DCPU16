#!perl 

use strict;
use warnings;
use CPU::Emulator::DCPU16::Disassembler;

my $file    = shift || die "You must pass in a object file";

open(my $fh, "<", $file) || die "Couldn't read file $file: $!";
my $program = do { local $/=undef; <$fh> };
close($fh);

print CPU::Emulator::DCPU16::Disassembler->dump($program);
